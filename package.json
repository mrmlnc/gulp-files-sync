{
  "name": "gulp-files-sync",
  "description": "A Gulp plugin providing one-way synchronization of directories with glob",
  "version": "0.3.2",
  "license": "MIT",
  "author": {
    "name": "Denis Malinochkin",
    "url": "canonium.com"
  },
  "repository": "mrmlnc/gulp-files-sync",
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "array-differ": "^1.0.0",
    "cp-file": "^3.1.0",
    "glob-parent": "^2.0.0",
    "globby": "^4.0.0",
    "gulp-util": "^3.0.7",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.0.1",
    "promise": "^7.1.1",
    "rimraf": "^2.5.0",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "mocha": "^2.3.4",
    "xo": "^0.10.1"
  },
  "scripts": {
    "test": "rimraf .tmp && xo && mocha --timeout=25000"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "keywords": [
    "gulpplugin",
    "directory",
    "dir",
    "files",
    "sync",
    "copy"
  ],
  "xo": {
    "space": 2,
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ],
      "arrow-parens": [
        2,
        "always"
      ]
    },
    "envs": [
      "node",
      "mocha"
    ]
  }
}
